import java.util.*;
class Biggest
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int a,b;
System.out.print("Enter A:");
a=sc.nextInt();
System.out.print("Enter B:");
b=sc.nextInt();
if(a<b){
System.out.print(b+"is bigger than "+a);
}
else{
System.out.print(a+"is bigger than"+b);
}
}
}
