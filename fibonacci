class Main {
  public static void main(String arg[])
{
int i = 1;
int n = 10;
int firstTerm = 0;
int secondTerm = 1;
    System.out.println("Fibonacci Series for"+n+"terms: ");
    while (i <= n) {
      System.out.print(firstTerm + ", ");
      int nextTerm = firstTerm + secondTerm;
      firstTerm = secondTerm;
      secondTerm = nextTerm;
      i++;
    }
  }
}
