import java.util.*;
class Vote
{
public static void main(String arg[])
{
Scanner sc=new Scanner(System.in);
int age;
System.out.print("Enter age:");
age=sc.nextInt();
if(age>=18){
System.out.print("The citizen is eligible to vote");
}
else{
System.out.print("The citizen is not eligible to vote");
}
}
}
